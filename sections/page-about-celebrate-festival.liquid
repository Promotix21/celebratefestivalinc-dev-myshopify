{% comment %}
  About Celebrate Festival Page Section - Celebrate Festival Inc
  A professional, elegant presentation of the company's story, products, and contact information
{% endcomment %}

<div class="about-celebrate-festival-page">
  <!-- Hero Section -->
  <section class="about-hero">
    <div class="about-hero__background">
      <div class="about-hero__shapes">
        <div class="shape shape--1"></div>
        <div class="shape shape--2"></div>
        <div class="shape shape--3"></div>
      </div>
    </div>
    <div class="about-hero__content">
      <div class="about-hero__tagline-pill">
        <span>{{ section.settings.hero_tagline_pill | default: 'Est. Since Day One' }}</span>
      </div>
      <h1 class="about-hero__title">{{ section.settings.hero_title | default: 'About Celebrate Festival Inc' }}</h1>
      <p class="about-hero__subtitle">{{ section.settings.hero_subtitle | default: 'Your Trusted Partner in Restaurant Equipment Excellence' }}</p>
      <div class="about-hero__cta">
        <a href="{{ section.settings.hero_cta_url | default: '/collections/all' }}" class="btn btn--primary">
          {{ section.settings.hero_cta_text | default: 'Explore Our Products' }}
        </a>
        <a href="#contact-section" class="btn btn--secondary">
          {{ section.settings.hero_secondary_cta | default: 'Contact Us' }}
        </a>
      </div>
    </div>
  </section>

  <!-- Welcome Section -->
  <section class="about-welcome">
    <div class="container">
      <div class="about-welcome__grid">
        <div class="about-welcome__content">
          <div class="section-label">
            <span class="section-label__bar"></span>
            <span class="section-label__text">{{ section.settings.welcome_label | default: 'Who We Are' }}</span>
          </div>
          <h2 class="about-welcome__title">{{ section.settings.welcome_title | default: 'Welcome to Celebrate Festival Inc' }}</h2>
          <div class="about-welcome__text">
            <p>{{ section.settings.welcome_paragraph_1 | default: 'Welcome to Celebrate Festival Inc - restaurant equipment company, your trusted partner in the world of restaurant equipment and solutions. With a passion for culinary excellence and a commitment to helping your food establishment thrive, we have been serving the needs of restaurateurs, bakers, deli owners, and culinary enthusiasts nationwide.' }}</p>
            <p>{{ section.settings.welcome_paragraph_2 | default: "At Celebrate Festival Inc, we understand that each restaurant, bakery, and deli is unique, with distinct requirements and aspirations. That's why we offer a comprehensive range of top-quality restaurant products tailored to suit every facet of your culinary venture. Whether you're setting up a cozy cafe, a bustling bakery, or an elegant fine-dining establishment, we've got you covered." }}</p>
          </div>
        </div>
        <div class="about-welcome__image">
          {% if section.settings.welcome_image %}
            {{ section.settings.welcome_image | image_url: width: 600 | image_tag: class: 'about-welcome__img', loading: 'lazy' }}
          {% else %}
            <div class="about-welcome__placeholder">
              <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1.5">
                <path d="M12 2L2 7l10 5 10-5-10-5zM2 17l10 5 10-5M2 12l10 5 10-5"/>
              </svg>
            </div>
          {% endif %}
        </div>
      </div>
    </div>
  </section>

  <!-- Expertise Section -->
  <section class="about-expertise">
    <div class="container">
      <div class="about-expertise__grid">
        <div class="about-expertise__image">
          {% if section.settings.expertise_image %}
            {{ section.settings.expertise_image | image_url: width: 600 | image_tag: class: 'about-expertise__img', loading: 'lazy' }}
          {% else %}
            <div class="about-expertise__placeholder">
              <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1.5">
                <path d="M12 3v18M3 12h18M5.5 5.5l13 13M18.5 5.5l-13 13"/>
              </svg>
            </div>
          {% endif %}
        </div>
        <div class="about-expertise__content">
          <div class="section-label">
            <span class="section-label__bar"></span>
            <span class="section-label__text">{{ section.settings.expertise_label | default: 'Our Expertise' }}</span>
          </div>
          <h2 class="about-expertise__title">{{ section.settings.expertise_title | default: 'Specialists in Indian Restaurant Equipment' }}</h2>
          <div class="about-expertise__text">
            <p>{{ section.settings.expertise_paragraph_1 | default: "While we take pride in catering to a wide spectrum of culinary businesses, our true expertise shines in a specific niche – the vibrant and diverse world of Indian restaurants. With a deep understanding of the intricacies of Indian cuisine and a commitment to sourcing authentic and innovative products, we have earned the trust of countless Indian restaurant owners and chefs across the country." }}</p>
            <p>{{ section.settings.expertise_paragraph_2 | default: "Our vast network of satisfied customers stands as a testament to our unwavering dedication to excellence." }}</p>
          </div>
          <div class="about-expertise__stats">
            <div class="stat-item">
              <span class="stat-item__number">{{ section.settings.stat_1_number | default: '1000+' }}</span>
              <span class="stat-item__label">{{ section.settings.stat_1_label | default: 'Satisfied Customers' }}</span>
            </div>
            <div class="stat-item">
              <span class="stat-item__number">{{ section.settings.stat_2_number | default: '500+' }}</span>
              <span class="stat-item__label">{{ section.settings.stat_2_label | default: 'Products Available' }}</span>
            </div>
            <div class="stat-item">
              <span class="stat-item__number">{{ section.settings.stat_3_number | default: '15+' }}</span>
              <span class="stat-item__label">{{ section.settings.stat_3_label | default: 'Years Experience' }}</span>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- Mission Section -->
  <section class="about-mission">
    <div class="container">
      <div class="about-mission__inner">
        <div class="section-label section-label--center">
          <span class="section-label__bar"></span>
          <span class="section-label__text">{{ section.settings.mission_label | default: 'Our Mission' }}</span>
        </div>
        <h2 class="about-mission__title">{{ section.settings.mission_title | default: 'Empowering Culinary Excellence' }}</h2>
        <p class="about-mission__text">{{ section.settings.mission_text | default: "At Celebrate Festival Inc, we believe that the success of your restaurant venture begins with equipping your kitchen with the finest tools and solutions. Our mission is to empower you to create exceptional culinary experiences for your patrons while optimizing your operational efficiency." }}</p>
        <p class="about-mission__cta-text">{{ section.settings.mission_cta_text | default: "Join the ranks of thousands of satisfied customers who have elevated their culinary endeavors with our products and expertise. Explore our extensive catalog, and let us be your partner in achieving culinary greatness." }}</p>
      </div>
    </div>
  </section>

  <!-- Products Section -->
  <section class="about-products">
    <div class="container">
      <div class="section-header">
        <div class="section-label section-label--center">
          <span class="section-label__bar"></span>
          <span class="section-label__text">{{ section.settings.products_label | default: 'What We Offer' }}</span>
        </div>
        <h2 class="section-title">{{ section.settings.products_title | default: 'Our Product Categories' }}</h2>
        <p class="section-subtitle">{{ section.settings.products_subtitle | default: 'Comprehensive solutions for every culinary need' }}</p>
      </div>
      <div class="about-products__grid">
        {% for block in section.blocks %}
          {% if block.type == 'product_category' %}
            <div class="product-category-card" {{ block.shopify_attributes }}>
              <div class="product-category-card__icon">
                {% if block.settings.icon_image %}
                  {{ block.settings.icon_image | image_url: width: 80 | image_tag: loading: 'lazy' }}
                {% else %}
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1.5">
                    <rect x="3" y="3" width="18" height="18" rx="2"/>
                    <path d="M12 8v8M8 12h8"/>
                  </svg>
                {% endif %}
              </div>
              <h3 class="product-category-card__title">{{ block.settings.title }}</h3>
              {% if block.settings.link_url != blank %}
                <a href="{{ block.settings.link_url }}" class="product-category-card__link">
                  Explore <span class="arrow">&#8594;</span>
                </a>
              {% endif %}
            </div>
          {% endif %}
        {% endfor %}
      </div>
      <div class="about-products__cta">
        <a href="{{ section.settings.products_cta_url | default: '/collections/all' }}" class="btn btn--primary btn--large">
          {{ section.settings.products_cta_text | default: 'View All Products' }}
        </a>
      </div>
    </div>
  </section>

  <!-- Contact Section -->
  <section class="about-contact" id="contact-section">
    <div class="container">
      <div class="about-contact__grid">
        <div class="about-contact__info">
          <div class="section-label">
            <span class="section-label__bar"></span>
            <span class="section-label__text">{{ section.settings.contact_label | default: 'Get In Touch' }}</span>
          </div>
          <h2 class="about-contact__title">{{ section.settings.contact_title | default: 'Contact Us' }}</h2>
          <p class="about-contact__intro">{{ section.settings.contact_intro | default: 'Products available online and in-store. Visit our warehouse conveniently located at the border of Milpitas & Fremont in CA.' }}</p>

          <div class="contact-details">
            <div class="contact-item">
              <div class="contact-item__icon">
                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
                  <path d="M21 10c0 7-9 13-9 13s-9-6-9-13a9 9 0 0 1 18 0z"/>
                  <circle cx="12" cy="10" r="3"/>
                </svg>
              </div>
              <div class="contact-item__content">
                <h4>Address</h4>
                <p>{{ section.settings.address | default: 'Celebrate Festival Inc, 235 Whitney Pl, Fremont, CA 94539' }}</p>
              </div>
            </div>

            <div class="contact-item">
              <div class="contact-item__icon">
                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
                  <path d="M22 16.92v3a2 2 0 0 1-2.18 2 19.79 19.79 0 0 1-8.63-3.07 19.5 19.5 0 0 1-6-6 19.79 19.79 0 0 1-3.07-8.67A2 2 0 0 1 4.11 2h3a2 2 0 0 1 2 1.72 12.84 12.84 0 0 0 .7 2.81 2 2 0 0 1-.45 2.11L8.09 9.91a16 16 0 0 0 6 6l1.27-1.27a2 2 0 0 1 2.11-.45 12.84 12.84 0 0 0 2.81.7A2 2 0 0 1 22 16.92z"/>
                </svg>
              </div>
              <div class="contact-item__content">
                <h4>Phone</h4>
                <p>
                  <a href="tel:{{ section.settings.phone_1 | default: '408-673-9999' }}">{{ section.settings.phone_1 | default: '408-673-9999' }}</a>
                  <span class="separator">or</span>
                  <a href="tel:{{ section.settings.phone_2 | default: '408-663-5929' }}">{{ section.settings.phone_2 | default: '408-663-5929' }}</a>
                </p>
              </div>
            </div>

            <div class="contact-item">
              <div class="contact-item__icon">
                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
                  <path d="M4 4h16c1.1 0 2 .9 2 2v12c0 1.1-.9 2-2 2H4c-1.1 0-2-.9-2-2V6c0-1.1.9-2 2-2z"/>
                  <polyline points="22,6 12,13 2,6"/>
                </svg>
              </div>
              <div class="contact-item__content">
                <h4>Email</h4>
                <p><a href="mailto:{{ section.settings.email | default: 'sales@celebratefestivalinc.com' }}">{{ section.settings.email | default: 'sales@celebratefestivalinc.com' }}</a></p>
              </div>
            </div>

            <div class="contact-item">
              <div class="contact-item__icon">
                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
                  <circle cx="12" cy="12" r="10"/>
                  <line x1="2" y1="12" x2="22" y2="12"/>
                  <path d="M12 2a15.3 15.3 0 0 1 4 10 15.3 15.3 0 0 1-4 10 15.3 15.3 0 0 1-4-10 15.3 15.3 0 0 1 4-10z"/>
                </svg>
              </div>
              <div class="contact-item__content">
                <h4>Websites</h4>
                <p>
                  <a href="{{ section.settings.website_1_url | default: 'https://www.celebratefestivalinc.com' }}" target="_blank">{{ section.settings.website_1 | default: 'www.celebratefestivalinc.com' }}</a>
                  <span class="separator">|</span>
                  <a href="{{ section.settings.website_2_url | default: 'https://www.buytandoor.com' }}" target="_blank">{{ section.settings.website_2 | default: 'BuyTandoor.Com' }}</a>
                </p>
              </div>
            </div>
          </div>
        </div>

        <div class="about-contact__directions">
          <div class="directions-card">
            <div class="directions-card__header">
              <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
                <polygon points="3 11 22 2 13 21 11 13 3 11"/>
              </svg>
              <h3>Directions</h3>
            </div>
            <div class="directions-card__content">
              <p>{{ section.settings.directions_text | default: '2-3 min from Fwy 880 exits (Mission or Dixon Landing), 1-2 min from Scott exit on 680. Less than 5 min from intersection of 237 & 880.' }}</p>
              <a href="{{ section.settings.map_url | default: 'https://maps.google.com/?q=235+Whitney+Pl,+Fremont,+CA+94539' }}" target="_blank" class="btn btn--outline">
                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" width="18" height="18">
                  <path d="M21 10c0 7-9 13-9 13s-9-6-9-13a9 9 0 0 1 18 0z"/>
                  <circle cx="12" cy="10" r="3"/>
                </svg>
                Get Directions
              </a>
            </div>
            {% if section.settings.map_embed %}
              <div class="directions-card__map">
                {{ section.settings.map_embed }}
              </div>
            {% else %}
              <div class="directions-card__map-placeholder">
                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1">
                  <rect x="2" y="2" width="20" height="20" rx="2"/>
                  <path d="M21 10c0 7-9 13-9 13s-9-6-9-13a9 9 0 0 1 18 0z" transform="scale(0.6) translate(8,6)"/>
                  <circle cx="12" cy="10" r="2"/>
                </svg>
                <p>Map placeholder - Add Google Maps embed in settings</p>
              </div>
            {% endif %}
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- Final CTA Section -->
  <section class="about-final-cta">
    <div class="container">
      <div class="about-final-cta__inner">
        <h2>{{ section.settings.final_cta_title | default: 'Ready to Elevate Your Kitchen?' }}</h2>
        <p>{{ section.settings.final_cta_text | default: 'Discover our extensive range of professional restaurant equipment and let us help you achieve culinary excellence.' }}</p>
        <div class="about-final-cta__buttons">
          <a href="{{ section.settings.final_cta_primary_url | default: '/collections/all' }}" class="btn btn--white">
            {{ section.settings.final_cta_primary_text | default: 'Shop Now' }}
          </a>
          <a href="tel:{{ section.settings.phone_1 | default: '408-673-9999' }}" class="btn btn--outline-white">
            {{ section.settings.final_cta_secondary_text | default: 'Call Us Today' }}
          </a>
        </div>
      </div>
    </div>
  </section>
</div>

<style>
  /* ========================================
     ABOUT US PAGE STYLES
     Celebrate Festival Inc Brand Colors
     ======================================== */

  :root {
    --cf-deep-blue: #1a365d;
    --cf-navy: #2d5a87;
    --cf-coral: #ff6b6b;
    --cf-gold: #d4af37;
    --cf-success: #10b981;
    --cf-text-dark: #0f172a;
    --cf-text-medium: #475569;
    --cf-text-light: #64748b;
    --cf-text-muted: #94a3b8;
    --cf-border: #e2e8f0;
    --cf-bg-light: #f8fafc;
    --cf-bg-white: #ffffff;
    --cf-overlay: rgba(26, 54, 93, 0.85);
  }

  /* Reset & Base */
  .about-celebrate-festival-page {
    font-family: 'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif;
    color: var(--cf-text-dark);
    line-height: 1.6;
  }

  .about-celebrate-festival-page * {
    box-sizing: border-box;
  }

  .about-celebrate-festival-page h1,
  .about-celebrate-festival-page h2,
  .about-celebrate-festival-page h3,
  .about-celebrate-festival-page h4 {
    font-family: 'Montserrat', -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif;
    font-weight: 700;
    line-height: 1.3;
    margin: 0;
  }

  .about-celebrate-festival-page p {
    margin: 0 0 1em;
  }

  .about-celebrate-festival-page p:last-child {
    margin-bottom: 0;
  }

  .about-celebrate-festival-page a {
    color: var(--cf-navy);
    text-decoration: none;
    transition: color 0.3s ease;
  }

  .about-celebrate-festival-page a:hover {
    color: var(--cf-coral);
  }

  .container {
    max-width: 1280px;
    margin: 0 auto;
    padding: 0 24px;
  }

  /* Section Label Component */
  .section-label {
    display: flex;
    align-items: center;
    gap: 12px;
    margin-bottom: 16px;
  }

  .section-label--center {
    justify-content: center;
  }

  .section-label__bar {
    width: 4px;
    height: 24px;
    background: linear-gradient(180deg, var(--cf-coral) 0%, var(--cf-navy) 100%);
    border-radius: 2px;
  }

  .section-label__text {
    font-family: 'Montserrat', sans-serif;
    font-size: 12px;
    font-weight: 700;
    text-transform: uppercase;
    letter-spacing: 2px;
    color: var(--cf-coral);
  }

  /* Section Header */
  .section-header {
    text-align: center;
    margin-bottom: 48px;
  }

  .section-title {
    font-size: 32px;
    color: var(--cf-deep-blue);
    margin-bottom: 16px;
  }

  .section-subtitle {
    font-size: 16px;
    color: var(--cf-text-medium);
    max-width: 600px;
    margin: 0 auto;
  }

  .about-products .section-subtitle {
    text-align: center;
    margin-left: auto;
    margin-right: auto;
  }

  /* Buttons */
  .btn {
    display: inline-flex;
    align-items: center;
    justify-content: center;
    gap: 8px;
    padding: 14px 28px;
    font-family: 'Montserrat', sans-serif;
    font-size: 14px;
    font-weight: 600;
    border-radius: 8px;
    cursor: pointer;
    transition: all 0.3s ease;
    border: 2px solid transparent;
    text-decoration: none;
  }

  .btn--primary {
    background: linear-gradient(135deg, var(--cf-coral) 0%, #ff5252 100%);
    color: white;
    border-color: var(--cf-coral);
  }

  .btn--primary:hover {
    background: linear-gradient(135deg, #ff5252 0%, var(--cf-coral) 100%);
    transform: translateY(-2px);
    box-shadow: 0 8px 20px rgba(255, 107, 107, 0.3);
    color: white;
  }

  .btn--secondary {
    background: transparent;
    color: white;
    border-color: rgba(255, 255, 255, 0.5);
  }

  .btn--secondary:hover {
    background: rgba(255, 255, 255, 0.1);
    border-color: white;
    color: white;
  }

  .btn--outline {
    background: transparent;
    color: var(--cf-navy);
    border-color: var(--cf-navy);
  }

  .btn--outline:hover {
    background: var(--cf-navy);
    color: white;
  }

  .btn--white {
    background: white;
    color: var(--cf-deep-blue);
    border-color: white;
  }

  .btn--white:hover {
    background: var(--cf-bg-light);
    transform: translateY(-2px);
    color: var(--cf-deep-blue);
  }

  .btn--outline-white {
    background: transparent;
    color: white;
    border-color: white;
  }

  .btn--outline-white:hover {
    background: rgba(255, 255, 255, 0.15);
    color: white;
  }

  .btn--large {
    padding: 16px 36px;
    font-size: 15px;
  }

  /* ========================================
     HERO SECTION
     ======================================== */

  .about-hero {
    position: relative;
    min-height: 420px;
    display: flex;
    align-items: center;
    justify-content: center;
    overflow: hidden;
    padding: 80px 24px;
  }

  .about-hero__background {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: linear-gradient(135deg, var(--cf-deep-blue) 0%, #0f172a 50%, var(--cf-navy) 100%);
    z-index: 0;
  }

  .about-hero__background::before {
    content: '';
    position: absolute;
    top: 0;
    left: -100%;
    width: 200%;
    height: 100%;
    background: linear-gradient(
      90deg,
      transparent 0%,
      rgba(255, 255, 255, 0.03) 25%,
      rgba(255, 255, 255, 0.05) 50%,
      rgba(255, 255, 255, 0.03) 75%,
      transparent 100%
    );
    animation: heroSweep 8s ease-in-out infinite;
  }

  @keyframes heroSweep {
    0%, 100% { transform: translateX(0); }
    50% { transform: translateX(50%); }
  }

  .about-hero__shapes {
    position: absolute;
    width: 100%;
    height: 100%;
    overflow: hidden;
  }

  .shape {
    position: absolute;
    border-radius: 50%;
    opacity: 0.08;
    background: white;
  }

  .shape--1 {
    width: 300px;
    height: 300px;
    top: -100px;
    right: -50px;
    animation: float1 15s ease-in-out infinite;
  }

  .shape--2 {
    width: 200px;
    height: 200px;
    bottom: -50px;
    left: 10%;
    animation: float2 12s ease-in-out infinite;
  }

  .shape--3 {
    width: 150px;
    height: 150px;
    top: 30%;
    left: -30px;
    animation: float3 10s ease-in-out infinite;
  }

  @keyframes float1 {
    0%, 100% { transform: translate(0, 0); }
    50% { transform: translate(-20px, 30px); }
  }

  @keyframes float2 {
    0%, 100% { transform: translate(0, 0); }
    50% { transform: translate(30px, -20px); }
  }

  @keyframes float3 {
    0%, 100% { transform: translate(0, 0); }
    50% { transform: translate(20px, 20px); }
  }

  .about-hero__content {
    position: relative;
    z-index: 1;
    text-align: center;
    max-width: 800px;
  }

  .about-hero__tagline-pill {
    display: inline-block;
    padding: 8px 20px;
    background: rgba(255, 107, 107, 0.2);
    border: 1px solid rgba(255, 107, 107, 0.3);
    border-radius: 50px;
    margin-bottom: 24px;
  }

  .about-hero__tagline-pill span {
    font-family: 'Montserrat', sans-serif;
    font-size: 12px;
    font-weight: 600;
    text-transform: uppercase;
    letter-spacing: 2px;
    color: var(--cf-coral);
  }

  .about-hero__title {
    font-size: 48px;
    color: white;
    margin-bottom: 20px;
    background: linear-gradient(135deg, #ffffff 0%, #e2e8f0 50%, var(--cf-gold) 100%);
