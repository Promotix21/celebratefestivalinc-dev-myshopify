<!-- sections/hexagon-grid.liquid -->
<style>
  .hexagon-section {
    padding: {{ section.settings.section_padding_desktop }}px 20px;
    background-color: {{ section.settings.background_color }};
    font-family: Arial, sans-serif;
  }

  .hexagon-container {
    max-width: 1400px;
    margin: 0 auto;
    text-align: center;
    padding: 0 20px;
  }

  .hexagon-title {
    color: {{ section.settings.title_color }};
    font-size: {{ section.settings.title_size_desktop }}px;
    font-weight: bold;
    margin-bottom: {{ section.settings.title_margin_desktop }}px;
  }

  .hexagon-grid {
    display: inline-block;
    position: relative;
    margin: 0 auto;
    --hex-size-desktop: {{ section.settings.hex_size_desktop }}px;
    --hex-height-desktop: {{ section.settings.hex_height_desktop }}px;
    --vertical-spacing-desktop: {{ section.settings.vertical_spacing_desktop }}px;
    --horizontal-spacing-desktop: {{ section.settings.horizontal_spacing_desktop }}px;
    --offset-desktop: {{ section.settings.honeycomb_offset_desktop }}px;
  }

  .hex-row {
    display: flex;
    justify-content: center;
    margin-bottom: calc(-1 * var(--vertical-spacing-desktop));
  }

  .hex-row:last-child {
    margin-bottom: 0;
  }

  /* Honeycomb offset for even rows */
  .hex-row:nth-child(even) {
    transform: translateX(var(--offset-desktop));
  }

  .hexagon-link {
    text-decoration: none;
    display: inline-block;
  }

  .hexagon-item {
    margin: 0 calc(-1 * var(--horizontal-spacing-desktop));
    cursor: pointer;
    transition: transform 0.3s ease;
  }

  .hexagon-item:hover {
    transform: scale({{ section.settings.hover_scale }});
    z-index: 999;
  }

  .hexagon-svg {
    width: var(--hex-size-desktop);
    height: var(--hex-height-desktop);
  }

  .hex-shape {
    fill: {{ section.settings.hex_background }};
    
    stroke-width: {{ section.settings.hex_border_width }};
  }

  .hex-logo {
    width: 100%;
    height: auto;
    max-height: 100%;
    object-fit: contain;
  }

  /* Stats Section Styling */
  .stats-section {
    margin-top: 60px;
    display: flex;
    justify-content: center;
    align-items: center;
    gap: 80px;
    flex-wrap: wrap;
  }

  .stat-item {
    text-align: center;
  }

  .stat-number {
    font-size: 48px;
    font-weight: bold;
    color: {{ section.settings.stats_number_color }};
    line-height: 1;
    margin-bottom: 5px;
  }

  .stat-label {
    font-size: 14px;
    color: {{ section.settings.stats_label_color }};
    font-weight: 500;
    text-transform: uppercase;
    letter-spacing: 1px;
  }

  .view-brands-btn {
    margin-top: 40px;
    display: inline-block;
    background-color: {{ section.settings.button_background_color }};
    color: {{ section.settings.button_text_color }};
    padding: 15px 30px;
    border-radius: 25px;
    text-decoration: none;
    font-weight: 600;
    font-size: 16px;
    transition: all 0.3s ease;
  }

  .view-brands-btn:hover {
    background-color: {{ section.settings.button_hover_color }};
    transform: translateY(-2px);
  }

  /* Tablet Responsive */
  @media screen and (max-width: 1024px) and (min-width: 768px) {
    .hexagon-section {
      padding: {{ section.settings.section_padding_tablet }}px 15px;
    }
    
    .hexagon-title {
      font-size: {{ section.settings.title_size_tablet }}px;
      margin-bottom: {{ section.settings.title_margin_tablet }}px;
    }
    
    .hexagon-grid {
      --hex-size-desktop: {{ section.settings.hex_size_tablet }}px;
      --hex-height-desktop: {{ section.settings.hex_height_tablet }}px;
      --vertical-spacing-desktop: {{ section.settings.vertical_spacing_tablet }}px;
      --horizontal-spacing-desktop: {{ section.settings.horizontal_spacing_tablet }}px;
      --offset-desktop: {{ section.settings.honeycomb_offset_tablet }}px;
    }

    .stats-section {
      gap: 60px;
      margin-top: 50px;
    }

    .stat-number {
      font-size: 40px;
    }

    .stat-label {
      font-size: 13px;
    }
  }

  /* Mobile Responsive */
  @media screen and (max-width: 767px) {
    .hexagon-section {
      padding: {{ section.settings.section_padding_mobile }}px 10px;
    }

    .hexagon-title {
      font-size: {{ section.settings.title_size_mobile }}px;
      margin-bottom: {{ section.settings.title_margin_mobile }}px;
    }

    .hexagon-grid {
      --hex-size-desktop: 68px;
      --hex-height-desktop: 59px;
      --vertical-spacing-desktop: 0px;
      --horizontal-spacing-desktop: 0px;
      --offset-desktop: 0px;
    }

    /* Center rows - honeycomb effect comes from 6-5-6-5 pattern */
    .hexagon-section .hexagon-grid .hex-row {
      justify-content: center !important;
      margin: 0 !important;
    }

    /* Row 2 */
    .hexagon-section .hexagon-grid .hex-row:nth-child(2) {
      transform: translateY(-18px) !important;
    }

    /* Row 3 */
    .hexagon-section .hexagon-grid .hex-row:nth-child(3) {
      transform: translateY(-36px) !important;
    }

    /* Row 4 */
    .hexagon-section .hexagon-grid .hex-row:nth-child(4) {
      transform: translateY(-54px) !important;
    }

    .hexagon-section .hexagon-grid .hexagon-item {
      margin: 0 -8px !important;
    }

    /* Hide hexagons on mobile - show only 4 per row */
    .hexagon-link.hide-on-mobile {
      display: none !important;
    }

    .stats-section {
      gap: 40px;
      margin-top: 40px;
    }

    .stat-number {
      font-size: 32px;
    }

    .stat-label {
      font-size: 12px;
    }

    .view-brands-btn {
      padding: 12px 25px;
      font-size: 15px;
    }
  }
</style>

<section class="hexagon-section">
