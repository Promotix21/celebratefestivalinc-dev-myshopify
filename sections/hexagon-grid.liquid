   <div class="hexagon-grid">
  <!-- Row 1: 8 hexagons (6 on mobile) -->
  <div class="hex-row">
    {% for i in (1..8) %}
      {% assign logo_key = 'logo_row1_' | append: i %}
      {% assign link_key = 'link_row1_' | append: i %}
      {% assign logo = section.settings[logo_key] %}
      {% assign link_url = section.settings[link_key] %}

      {%- assign brand_idx = forloop.index0 -%}
      {%- assign default_brand_link = brand_links[brand_idx] | default: '/pages/brands' -%}
      {% assign hex_link = link_url | default: default_brand_link %}
      <a href="{{ hex_link }}" class="hexagon-link{% if i > 6 %} hide-on-mobile{% endif %}">
        <div class="hexagon-item">
          <svg class="hexagon-svg" viewBox="0 0 80 70">
            <defs>
              <linearGradient id="hexBorderGradient-row1-{{ i }}" x1="0%" y1="0%" x2="100%" y2="100%">
                <stop offset="0%" stop-color="{{ section.settings.hex_border_gradient_start }}" />
                <stop offset="100%" stop-color="{{ section.settings.hex_border_gradient_end }}" />
              </linearGradient>
            </defs>
            <polygon class="hex-shape"
              points="40,2 70,18 70,52 40,68 10,52 10,18"
              fill="{{ section.settings.hex_background }}"
              stroke="url(#hexBorderGradient-row1-{{ i }})"
              stroke-width="{{ section.settings.hex_border_width }}" />
            <foreignObject x="15" y="25" width="50" height="40">
              {% if logo != blank %}
                <img class="hex-logo" src="{{ logo | img_url: '200x200' }}" alt="Brand Logo {{ i }}" loading="lazy">
              {% else %}
                <img class="hex-logo" src="https://cf-images.hiraya.digital/emails/4th-july/images/footer_logo.png" alt="Placeholder Logo" loading="lazy">
              {% endif %}
            </foreignObject>
          </svg>
        </div>
      </a>
    {% endfor %}
  </div>

  <!-- Row 2: 9 hexagons (5 on mobile) -->
  <div class="hex-row">
    {% for i in (1..9) %}
      {% assign logo_key = 'logo_row2_' | append: i %}
