<!-- sections/featured-categories.liquid -->
<style>
  .featured-categories-{{ section.id }} {
    padding: {{ section.settings.section_padding_top_desktop }}px 0 {{ section.settings.section_padding_bottom_desktop }}px;
    background: {{ section.settings.section_background_color }};
  }
  
  @media (max-width: 768px) {
    .featured-categories-{{ section.id }} {
      padding: {{ section.settings.section_padding_top_mobile }}px 0 {{ section.settings.section_padding_bottom_mobile }}px;
    }
  }
  
  .featured-categories-{{ section.id }} .container {
    max-width: {{ section.settings.container_max_width }}px;
    margin: 0 auto;
    padding: 0 {{ section.settings.container_padding }}px;
  }
  
  .featured-categories-{{ section.id }} .section-title {
    text-align: {{ section.settings.title_alignment }};
    margin-bottom: {{ section.settings.title_margin_bottom }}px;
    font-family: {{ section.settings.title_font_family }};
    font-size: {{ section.settings.title_font_size_desktop }}px;
    line-height: {{ section.settings.title_line_height }};
    font-weight: {{ section.settings.title_font_weight }};
    color: {{ section.settings.title_color }};
  }
  
  @media (max-width: 768px) {
    .featured-categories-{{ section.id }} .section-title {
      font-size: {{ section.settings.title_font_size_mobile }}px;
    }
  }
  
  .featured-categories-{{ section.id }} .title-accent {
    display: block;
    font-size: {{ section.settings.subtitle_font_size_desktop }}px;
    font-weight: {{ section.settings.subtitle_font_weight }};
    color: {{ section.settings.subtitle_color }};
    margin-top: {{ section.settings.subtitle_margin_top }}px;
    font-family: {{ section.settings.subtitle_font_family }};
    opacity: {{ section.settings.subtitle_opacity | divided_by: 100.0 }};
  }
  
  @media (max-width: 768px) {
    .featured-categories-{{ section.id }} .title-accent {
      font-size: {{ section.settings.subtitle_font_size_mobile }}px;
    }
  }
  
  .featured-categories-{{ section.id }} .categories-grid {
    display: grid;
    grid-template-columns: repeat(4, 1fr);
    gap: {{ section.settings.grid_gap_desktop }}px;
    margin-bottom: {{ section.settings.grid_margin_bottom }}px;
  }
  
  @media (max-width: 992px) {
    .featured-categories-{{ section.id }} .categories-grid {
      grid-template-columns: repeat(4, 1fr);
      gap: {{ section.settings.grid_gap_tablet }}px;
    }
  }
  
  @media (max-width: 768px) {
    .featured-categories-{{ section.id }} .categories-grid {
      grid-template-columns: repeat(2, 1fr);
      gap: {{ section.settings.grid_gap_mobile }}px;
    }
  }
  
  .featured-categories-{{ section.id }} .category-card {
    position: relative;
    display: block;
    text-decoration: none;
    background: {{ section.settings.card_background_color }};
    border-radius: {{ section.settings.card_border_radius }}px;
    overflow: hidden;
    transition: all {{ section.settings.card_hover_transition }}s ease;
    border: {{ section.settings.card_border_width }}px solid {{ section.settings.card_border_color }};
    box-shadow: {{ section.settings.card_shadow }};
    height: {{ section.settings.card_height_desktop }}px;
  }
  
  @media (max-width: 768px) {
    .featured-categories-{{ section.id }} .category-card {
      height: {{ section.settings.card_height_mobile }}px;
    }
  }
  
  .featured-categories-{{ section.id }} .category-card:hover {
    transform: translateY({{ section.settings.card_hover_translate }}px) scale({{ section.settings.card_hover_scale }});
    box-shadow: {{ section.settings.card_hover_shadow }};
  }
  
  .featured-categories-{{ section.id }} .category-image {
    position: relative;
    width: 100%;
    height: 70%;
    overflow: hidden;
  }
  
  .featured-categories-{{ section.id }} .category-image img {
    width: 100%;
    height: 100%;
    object-fit: cover;
    transition: transform {{ section.settings.image_hover_transition }}s ease;
  }
  
  .featured-categories-{{ section.id }} .category-card:hover .category-image img {
    transform: scale({{ section.settings.image_hover_scale }});
  }
  
  /* Brand Logos Overlay Styles */
  .featured-categories-{{ section.id }} .category-overlay {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: {{ section.settings.overlay_background }};
    {% if section.settings.overlay_gradient_enabled %}
      background: {{ section.settings.overlay_gradient }};
    {% endif %}
    opacity: 0;
    transition: opacity {{ section.settings.overlay_transition_duration }}s ease;
    display: flex;
    align-items: center;
    justify-content: center;
    z-index: 1;
    pointer-events: none;
  }
  
  .featured-categories-{{ section.id }} .category-card:hover .category-overlay {
    opacity: {{ section.settings.overlay_opacity | divided_by: 100.0 }};
  }
  
  .featured-categories-{{ section.id }} .logos-grid {
    display: grid;
    grid-template-columns: repeat({{ section.settings.logos_per_row }}, 1fr);
    gap: {{ section.settings.logos_gap }}px;
    padding: {{ section.settings.logos_padding }}px;
    max-width: {{ section.settings.logos_container_width }}%;
    width: 100%;
  }
  
  @media (max-width: 768px) {
    .featured-categories-{{ section.id }} .logos-grid {
      grid-template-columns: repeat({{ section.settings.logos_per_row_mobile }}, 1fr);
      gap: {{ section.settings.logos_gap_mobile }}px;
      padding: {{ section.settings.logos_padding_mobile }}px;
    }
  }
  
  .featured-categories-{{ section.id }} .brand-logo {
    width: 100%;
    height: auto;
    max-width: {{ section.settings.logo_max_width }}px;
    object-fit: contain;
    filter: grayscale({{ section.settings.logo_grayscale }}%) brightness({{ section.settings.logo_brightness }}%);
    transition: all {{ section.settings.logo_hover_transition }}s ease;
    opacity: {{ section.settings.logo_opacity | divided_by: 100.0 }};
  }
  
  @media (max-width: 768px) {
    .featured-categories-{{ section.id }} .brand-logo {
      max-width: {{ section.settings.logo_max_width_mobile }}px;
    }
  }
  
  .featured-categories-{{ section.id }} .category-card:hover .brand-logo {
    filter: grayscale({{ section.settings.logo_hover_grayscale }}%) brightness({{ section.settings.logo_hover_brightness }}%);
    transform: scale({{ section.settings.logo_hover_scale }});
  }
  
  .featured-categories-{{ section.id }} .category-badge {
    position: absolute;
    top: {{ section.settings.badge_top }}px;
    right: {{ section.settings.badge_right }}px;
    padding: {{ section.settings.badge_padding_y }}px {{ section.settings.badge_padding_x }}px;
    border-radius: {{ section.settings.badge_border_radius }}px;
    font-size: {{ section.settings.badge_font_size }}px;
    font-weight: {{ section.settings.badge_font_weight }};
    text-transform: uppercase;
    letter-spacing: {{ section.settings.badge_letter_spacing }}px;
    z-index: 2;
  }
  
  .featured-categories-{{ section.id }} .sale-badge {
    background: {{ section.settings.sale_badge_bg }};
    color: {{ section.settings.sale_badge_color }};
  }
  
  .featured-categories-{{ section.id }} .new-badge {
    background: {{ section.settings.new_badge_bg }};
    color: {{ section.settings.new_badge_color }};
  }
  
  .featured-categories-{{ section.id }} .hot-badge {
    background: {{ section.settings.hot_badge_bg }};
    color: {{ section.settings.hot_badge_color }};
  }
  
  .featured-categories-{{ section.id }} .category-info {
    height: 30%;
    padding: {{ section.settings.card_info_padding }}px;
    display: flex;
    align-items: center;
    justify-content: {{ section.settings.card_text_alignment }};
    background: {{ section.settings.card_info_background }};
  }
  
  .featured-categories-{{ section.id }} .category-info h3 {
    font-size: {{ section.settings.category_title_font_size_desktop }}px;
    line-height: {{ section.settings.category_title_line_height }};
    font-weight: {{ section.settings.category_title_font_weight }};
    color: {{ section.settings.category_title_color }};
    margin: 0;
    font-family: {{ section.settings.category_title_font_family }};
    text-align: {{ section.settings.card_text_alignment }};
    transition: color {{ section.settings.category_title_hover_transition }}s ease;
  }
  
  @media (max-width: 768px) {
    .featured-categories-{{ section.id }} .category-info h3 {
      font-size: {{ section.settings.category_title_font_size_mobile }}px;
    }
  }
  
  .featured-categories-{{ section.id }} .category-card:hover .category-info h3 {
    color: {{ section.settings.category_title_hover_color }};
  }
  
  .featured-categories-{{ section.id }} .hidden-category {
    display: none;
    animation: fadeOut {{ section.settings.animation_duration }}s ease;
  }
  
  .featured-categories-{{ section.id }} .show-category {
    display: block;
    animation: fadeIn {{ section.settings.animation_duration }}s ease;
  }
  
  @keyframes fadeIn {
    from {
      opacity: 0;
      transform: translateY(20px);
    }
    to {
      opacity: 1;
      transform: translateY(0);
    }
  }
  
  @keyframes fadeOut {
    from {
      opacity: 1;
      transform: translateY(0);
    }
    to {
      opacity: 0;
      transform: translateY(-20px);
    }
  }
  
  .featured-categories-{{ section.id }} .view-all {
    text-align: {{ section.settings.button_alignment }};
    margin-top: {{ section.settings.button_margin_top }}px;
  }
  
  .featured-categories-{{ section.id }} .expand-btn {
    background: {{ section.settings.button_background_color }};
    color: {{ section.settings.button_text_color }};
    border: {{ section.settings.button_border_width }}px solid {{ section.settings.button_border_color }};
    padding: {{ section.settings.button_padding_y }}px {{ section.settings.button_padding_x }}px;
    border-radius: {{ section.settings.button_border_radius }}px;
    font-size: {{ section.settings.button_font_size }}px;
    font-weight: {{ section.settings.button_font_weight }};
    font-family: {{ section.settings.button_font_family }};
    cursor: pointer;
    transition: all {{ section.settings.button_hover_transition }}s ease;
    display: inline-flex;
    align-items: center;
    gap: {{ section.settings.button_icon_gap }}px;
    text-decoration: none;
  }
  
  .featured-categories-{{ section.id }} .expand-btn:hover {
    background: {{ section.settings.button_hover_background_color }};
    color: {{ section.settings.button_hover_text_color }};
    transform: translateY({{ section.settings.button_hover_translate }}px);
    border-color: {{ section.settings.button_hover_border_color }};
  }
  
  .featured-categories-{{ section.id }} .expand-btn.expanded .btn-icon {
    transform: rotate(180deg);
  }
  
  .featured-categories-{{ section.id }} .btn-icon {
    transition: transform {{ section.settings.button_icon_transition }}s ease;
  }
</style>

<section class="featured-categories featured-categories-{{ section.id }}">
  <div class="container">
    {% if section.settings.show_title %}
      <h2 class="section-title">
        {{ section.settings.section_title }}
        {% if section.settings.show_subtitle and section.settings.section_subtitle != blank %}
          <span class="title-accent">{{ section.settings.section_subtitle }}</span>
        {% endif %}
      </h2>
    {% endif %}
    
    <div class="categories-grid" id="categoriesGrid-{{ section.id }}">
      {% assign visible_count = 0 %}
      {% for block in section.blocks %}
        {% case block.type %}
          {% when 'category' %}
            {% assign visible_count = visible_count | plus: 1 %}
            <a href="{{ block.settings.category_link | default: '#' }}" 
               class="category-card {% unless block.settings.visible_by_default %}hidden-category{% endunless %}" 
               data-id="{{ block.id }}"
               {{ block.shopify_attributes }}>
              <div class="category-image">
                {% if block.settings.category_image %}
                  <img src="{{ block.settings.category_image | img_url: '400x300' }}" 
                       alt="{{ block.settings.category_name }}" 
                       loading="lazy">
                {% else %}
                  <div style="background: #f0f0f0; width: 100%; height: 100%; display: flex; align-items: center; justify-content: center; color: #999;">
                    No Image
                  </div>
                {% endif %}
                
                {% if block.settings.show_badge and block.settings.badge_text != blank %}
                  <div class="category-badge {{ block.settings.badge_type }}-badge">
                    {{ block.settings.badge_text }}
                  </div>
                {% endif %}
                
                {% comment %} Brand Logos Overlay {% endcomment %}
                {% if block.settings.show_brand_overlay %}
                  <div class="category-overlay">
                    <div class="logos-grid">
                      {% if block.settings.brand_logo_1 %}
                        <img src="{{ block.settings.brand_logo_1 | img_url: '120x120' }}" 
                             alt="Brand Logo" 
                             class="brand-logo"
                             loading="lazy">
                      {% endif %}
                      {% if block.settings.brand_logo_2 %}
                        <img src="{{ block.settings.brand_logo_2 | img_url: '120x120' }}" 
                             alt="Brand Logo" 
                             class="brand-logo"
                             loading="lazy">
                      {% endif %}
                      {% if block.settings.brand_logo_3 %}
                        <img src="{{ block.settings.brand_logo_3 | img_url: '120x120' }}" 
                             alt="Brand Logo" 
                             class="brand-logo"
                             loading="lazy">
                      {% endif %}
                      {% if block.settings.brand_logo_4 %}
                        <img src="{{ block.settings.brand_logo_4 | img_url: '120x120' }}" 
                             alt="Brand Logo" 
                             class="brand-logo"
                             loading="lazy">
                      {% endif %}
                      {% if block.settings.brand_logo_5 %}
                        <img src="{{ block.settings.brand_logo_5 | img_url: '120x120' }}" 
                             alt="Brand Logo" 
                             class="brand-logo"
                             loading="lazy">
                      {% endif %}
                      {% if block.settings.brand_logo_6 %}
                        <img src="{{ block.settings.brand_logo_6 | img_url: '120x120' }}" 
                             alt="Brand Logo" 
                             class="brand-logo"
                             loading="lazy">
                      {% endif %}
                      {% if block.settings.brand_logo_7 %}
                        <img src="{{ block.settings.brand_logo_7 | img_url: '120x120' }}" 
                             alt="Brand Logo" 
                             class="brand-logo"
                             loading="lazy">
                      {% endif %}
                      {% if block.settings.brand_logo_8 %}
                        <img src="{{ block.settings.brand_logo_8 | img_url: '120x120' }}" 
                             alt="Brand Logo" 
                             class="brand-logo"
                             loading="lazy">
                      {% endif %}
                    </div>
                  </div>
                {% endif %}
              </div>
              <div class="category-info">
                <h3>{{ block.settings.category_name }}</h3>
              </div>
            </a>
        {% endcase %}
      {% endfor %}
    </div>
    
    {% if section.settings.show_expand_button %}
      <div class="view-all">
        <button class="expand-btn" id="expandCategoriesBtn-{{ section.id }}">
          <span class="btn-text">{{ section.settings.expand_button_text }}</span>
          <i class="fas fa-chevron-down btn-icon"></i>
        </button>
      </div>
    {% endif %}
  </div>
</section>

<script>
document.addEventListener('DOMContentLoaded', function() {
  const expandBtn = document.getElementById('expandCategoriesBtn-{{ section.id }}');
  const grid = document.getElementById('categoriesGrid-{{ section.id }}');
  let isExpanded = false;
  
  if (expandBtn && grid) {
    expandBtn.addEventListener('click', function() {
      const btnText = this.querySelector('.btn-text');
      const hiddenCategories = grid.querySelectorAll('.hidden-category');
      
      if (!isExpanded) {
        // Expand categories
        hiddenCategories.forEach((category, index) => {
          setTimeout(() => {
            category.classList.remove('hidden-category');
            category.classList.add('show-category');
          }, index * {{ section.settings.stagger_animation_delay }});
        });
        
        btnText.textContent = '{{ section.settings.collapse_button_text }}';
        this.classList.add('expanded');
        isExpanded = true;
        
      } else {
        // Collapse categories
        const visibleHidden = grid.querySelectorAll('.show-category');
        Array.from(visibleHidden).reverse().forEach((category, index) => {
          setTimeout(() => {
            category.classList.remove('show-category');
            category.classList.add('hidden-category');
          }, index * {{ section.settings.stagger_animation_delay | divided_by: 2 }});
        });
        
        btnText.textContent = '{{ section.settings.expand_button_text }}';
        this.classList.remove('expanded');
        isExpanded = false;
      }
    });
  }
});
</script>

{% schema %}
{
  "name": "Featured Categories",
  "tag": "section",
  "class": "section-featured-categories",
  "max_blocks": 20,
  "settings": [
    {
      "type": "header",
      "content": "Section Layout"
    },
    {
      "type": "range",
      "id": "section_padding_top_desktop",
      "min": 0,
      "max": 200,
      "step": 5,
      "unit": "px",
      "label": "Top padding (Desktop)",
      "default": 80
    },
    {
      "type": "range",
      "id": "section_padding_bottom_desktop",
      "min": 0,
      "max": 200,
      "step": 5,
      "unit": "px",
      "label": "Bottom padding (Desktop)",
      "default": 80
    },
    {
      "type": "range",
      "id": "section_padding_top_mobile",
      "min": 0,
      "max": 150,
      "step": 5,
      "unit": "px",
      "label": "Top padding (Mobile)",
      "default": 50
    },
    {
      "type": "range",
      "id": "section_padding_bottom_mobile",
      "min": 0,
      "max": 150,
      "step": 5,
      "unit": "px",
      "label": "Bottom padding (Mobile)",
      "default": 50
    },
    {
      "type": "color",
      "id": "section_background_color",
      "label": "Section background color",
      "default": "#ffffff"
    },
    {
      "type": "range",
      "id": "container_max_width",
      "min": 1000,
      "max": 1600,
      "step": 50,
      "unit": "px",
      "label": "Container max width",
      "default": 1200
    },
    {
      "type": "range",
      "id": "container_padding",
      "min": 15,
      "max": 50,
      "step": 5,
      "unit": "px",
      "label": "Container side padding",
      "default": 20
    },
    {
      "type": "header",
      "content": "Section Title"
    },
    {
      "type": "checkbox",
      "id": "show_title",
      "label": "Show section title",
      "default": true
    },
    {
      "type": "text",
      "id": "section_title",
      "label": "Section title",
      "default": "Featured Categories"
    },
    {
      "type": "checkbox",
      "id": "show_subtitle",
      "label": "Show subtitle",
      "default": true
    },
    {
      "type": "text",
      "id": "section_subtitle",
      "label": "Section subtitle",
      "default": "Unlimited Products. Endless Possibilities."
    },
    {
      "type": "select",
      "id": "title_alignment",
      "label": "Title alignment",
      "options": [
        { "value": "left", "label": "Left" },
        { "value": "center", "label": "Center" },
        { "value": "right", "label": "Right" }
      ],
      "default": "center"
    },
    {
      "type": "font_picker",
      "id": "title_font_family",
      "label": "Title font family",
      "default": "helvetica_n4"
    },
    {
      "type": "range",
      "id": "title_font_size_desktop",
      "min": 24,
      "max": 72,
      "step": 2,
      "unit": "px",
      "label": "Title font size (Desktop)",
      "default": 36
    },
    {
      "type": "range",
      "id": "title_font_size_mobile",
      "min": 20,
      "max": 48,
      "step": 2,
      "unit": "px",
      "label": "Title font size (Mobile)",
      "default": 28
    },
    {
      "type": "select",
      "id": "title_font_weight",
      "label": "Title font weight",
      "options": [
        { "value": "400", "label": "Normal" },
        { "value": "500", "label": "Medium" },
        { "value": "600", "label": "Semi Bold" },
        { "value": "700", "label": "Bold" },
        { "value": "800", "label": "Extra Bold" }
      ],
      "default": "700"
    },
    {
      "type": "range",
      "id": "title_line_height",
      "min": 1.0,
      "max": 2.0,
      "step": 0.1,
      "label": "Title line height",
      "default": 1.2
    },
    {
      "type": "color",
      "id": "title_color",
      "label": "Title color",
      "default": "#2d3748"
    },
    {
      "type": "range",
      "id": "title_margin_bottom",
      "min": 20,
      "max": 80,
      "step": 5,
      "unit": "px",
      "label": "Title margin bottom",
      "default": 50
    },
    {
      "type": "header",
      "content": "Section Subtitle"
    },
    {
      "type": "font_picker",
      "id": "subtitle_font_family",
      "label": "Subtitle font family",
      "default": "helvetica_n4"
    },
    {
      "type": "range",
      "id": "subtitle_font_size_desktop",
      "min": 14,
      "max": 32,
      "step": 1,
      "unit": "px",
      "label": "Subtitle font size (Desktop)",
      "default": 18
    },
    {
      "type": "range",
      "id": "subtitle_font_size_mobile",
      "min": 12,
      "max": 24,
      "step": 1,
      "unit": "px",
      "label": "Subtitle font size (Mobile)",
      "default": 16
    },
    {
      "type": "select",
      "id": "subtitle_font_weight",
      "label": "Subtitle font weight",
      "options": [
        { "value": "300", "label": "Light" },
        { "value": "400", "label": "Normal" },
        { "value": "500", "label": "Medium" },
        { "value": "600", "label": "Semi Bold" }
      ],
      "default": "400"
    },
    {
      "type": "color",
      "id": "subtitle_color",
      "label": "Subtitle color",
      "default": "#718096"
    },
    {
      "type": "range",
      "id": "subtitle_margin_top",
      "min": 5,
      "max": 30,
      "step": 1,
      "unit": "px",
      "label": "Subtitle margin top",
      "default": 10
    },
    {
      "type": "range",
      "id": "subtitle_opacity",
      "min": 50,
      "max": 100,
      "step": 5,
      "unit": "%",
      "label": "Subtitle opacity",
      "default": 80
    },
    {
      "type": "header",
      "content": "Brand Logos Overlay"
    },
    {
      "type": "color",
      "id": "overlay_background",
      "label": "Overlay background color",
      "default": "rgba(255, 255, 255, 0.95)"
    },
    {
      "type": "checkbox",
      "id": "overlay_gradient_enabled",
      "label": "Use gradient overlay",
      "default": false
    },
    {
      "type": "text",
      "id": "overlay_gradient",
      "label": "Overlay gradient",
      "default": "linear-gradient(135deg, rgba(255,255,255,0.95) 0%, rgba(248,250,252,0.9) 100%)",
      "info": "CSS gradient value (only applies if gradient is enabled)"
    },
    {
      "type": "range",
      "id": "overlay_opacity",
      "min": 70,
      "max": 100,
      "step": 5,
      "unit": "%",
      "label": "Overlay opacity on hover",
      "default": 95
    },
    {
      "type": "range",
      "id": "overlay_transition_duration",
      "min": 0.2,
      "max": 1.0,
      "step": 0.1,
      "unit": "s",
      "label": "Overlay transition duration",
      "default": 0.4
